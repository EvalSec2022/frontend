{
	"name": "frontend",
	"version": "0.1.0",
	"private": true,
	"dependencies": {
		"@chakra-ui/react": "^2.5.5",
		"@craco/craco": "^7.0.0",
		"@emotion/react": "^11.10.6",
		"@emotion/styled": "^11.10.6",
		"@headlessui/react": "^1.7.7",
		"@headlessui/tailwindcss": "^0.1.2",
		"@kunukn/react-collapse": "^2.2.10",
		"@ngneat/falso": "^6.4.0",
		"@tanstack/react-query": "^4.29.5",
		"@tanstack/react-query-devtools": "^4.29.5",
		"@testing-library/jest-dom": "^5.16.5",
		"@testing-library/react": "^13.4.0",
		"@testing-library/user-event": "^13.5.0",
		"@types/jest": "^27.5.2",
		"@types/node": "^16.18.11",
		"@types/react": "^18.0.27",
		"@types/react-dom": "^18.0.10",
		"axios": "^1.3.6",
		"daisyui": "^2.47.0",
		"framer-motion": "^10.12.2",
		"lint-staged": "^13.1.0",
		"prettier": "^2.8.3",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"react-query-auth": "^2.2.0",
		"react-router-dom": "^6.7.0",
		"react-scripts": "5.0.1",
		"typescript": "^4.9.4",
		"usehooks-ts": "^2.9.1",
		"web-vitals": "^2.1.4"
	},
	"scripts": {
		"start": "craco start",
		"build": "craco build",
		"test": "craco test",
		"eject": "react-scripts eject",
		"//": "New scripts to run and build Storybook with Tailwind",
		"storybook": "concurrently \"yarn:watch:*\"",
		"poststorybook": "npm run clean",
		"clean": " node ./clean src/tailwind.css",
		"build-storybook": "concurrently \"yarn:build:*\"",
		"build:css": "npx tailwindcss -i ./src/index.css -o ./src/tailwind.css",
		"build:storybook": "build-storybook build",
		"watch:css": "npx tailwindcss -i ./src/index.css -o ./src/tailwind.css --watch",
		"watch:storybook": "start-storybook dev -p 6006"
	},
	"eslintConfig": {
		"extends": [
			"react-app",
			"react-app/jest"
		],
		"overrides": [
			{
				"files": [
					"**/*.stories.*"
				],
				"rules": {
					"import/no-anonymous-default-export": "off"
				}
			}
		]
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
	"devDependencies": {
		"@storybook/addon-actions": "^6.5.15",
		"@storybook/addon-essentials": "^6.5.15",
		"@storybook/addon-interactions": "^6.5.15",
		"@storybook/addon-links": "^6.5.15",
		"@storybook/builder-webpack5": "^6.5.15",
		"@storybook/manager-webpack5": "^6.5.15",
		"@storybook/node-logger": "^6.5.15",
		"@storybook/preset-create-react-app": "^4.1.2",
		"@storybook/react": "^6.5.15",
		"@storybook/testing-library": "^0.0.13",
		"@tailwindcss/forms": "^0.5.3",
		"@tailwindcss/line-clamp": "^0.4.2",
		"autoprefixer": "^10.4.13",
		"babel-plugin-named-exports-order": "^0.0.2",
		"concurrently": "^7.6.0",
		"husky": "^4.3.8",
		"postcss": "^8.4.21",
		"prop-types": "^15.8.1",
		"tailwindcss": "^3.2.4",
		"webpack": "^5.75.0"
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged"
		}
	},
	"lint-staged": {
		"*.{js,jsx,ts,tsx,json,css,md}": [
			"prettier --write"
		]
	},
	"cracoConfig": "craco.config.js"
}
